	<subDataset name="RefereeMonitoring" uuid="63b75b61-82b2-4eef-b26d-4efc9837d40a">
		<parameter name="subFromDate" class="java.lang.String"/>
		<parameter name="subToDate" class="java.lang.String"/>
		<parameter name="thematicAreaId" class="java.lang.String"/>
		<parameter name="maj_area" class="java.lang.String"/>
		<queryString>
			<![CDATA[select
get_user_name(str_assigned_to),get_themetic_area_name(str_assigned_to),
count(*),
(select count(*) from tdc_proposal_tracker b where b.str_assigned_to=a.str_assigned_to and b.dt_allotment_date between to_date(nvl($P{subFromDate},'01-01-2012') ,'dd-mm-yyyy') and nvl(to_date($P{subToDate} ,'dd-mm-yyyy'),sysdate) and str_decision_date is not null and str_status not in ('CE'))  from
tdc_proposal_tracker a where dt_allotment_date between to_date(nvl($P{subFromDate},'01-01-2012') ,'dd-mm-yyyy') and nvl(to_date($P{subToDate} ,'dd-mm-yyyy'),sysdate) and
 (select NUM_TREMATIC_AREA_ID from tdc_proposal_master where NUM_PROPOSAL_ID = A.NUM_PROPOSAL_ID ) like upper('%'||$P{thematicAreaId}||'%')
group by str_assigned_to
order by 3 desc]]>
		</queryString>
		<field name="get_user_name" class="java.lang.String"/>
		<field name="get_themetic_area_name" class="java.lang.String"/>
		<field name="count" class="java.lang.Long"/>
		<field name="?column?" class="java.lang.Long"/>
	</subDataset>
	<parameter name="subFromDate" class="java.lang.String">
		<defaultValueExpression><![CDATA["01-01-2012"]]></defaultValueExpression>
	</parameter>
